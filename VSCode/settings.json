{
  "editor.fontFamily": "Fira Code",
  "editor.fontSize": 13,
  "files.autoSave": "onFocusChange",
  "workbench.preferredDarkColorTheme": "Community Material Theme Ocean High Contrast",
  "workbench.preferredLightColorTheme": "Solarized Light",
  "editor.scrollBeyondLastLine": true,
  "editor.wordWrap": "on",
  "editor.formatOnPaste": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "workbench.colorTheme": "Community Material Theme Ocean High Contrast",
  "editor.tabSize": 2,
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "telemetry.enableCrashReporter": false,
  "workbench.tree.indent": 18,
  "editor.renderWhitespace": "all",
  "editor.renderControlCharacters": true,
  "explorer.openEditors.visible": 0,
  "window.zoomLevel": 0,
  "window.titleBarStyle": "custom",
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.lineHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "files.associations": {
    "*.tsx": "typescriptreact"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "material-icon-theme",
  "files.exclude": {
    "**/.rts2_cache*/": true
  },
  "search.exclude": {
    "**/dist": true
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "debug.onTaskErrors": "showErrors",
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.scrollToSwitchTabs": true,
  "gitlens.hovers.currentLine.over": "line",
  "cSpell.userWords": [
    "DEVTOOLS",
    "Diwaker's",
    "Microblock",
    "behaviour",
    "bignumber",
    "bitcoinjs",
    "blockstack",
    "checkmark",
    "ciphertext",
    "devtool",
    "diwaker",
    "esnext",
    "formik",
    "homoglyph",
    "keychain",
    "keypairs",
    "kyr",
    "kyran",
    "kyr–∞njamie",
    "mocknet",
    "njamie",
    "pageview",
    "persistor",
    "qrcode",
    "reauthenticate",
    "reduxjs",
    "repr",
    "slugify",
    "submenu",
    "tilda",
    "track",
    "triplesec",
    "txid",
    "unstyled",
    "upsert",
    "urljoin",
    "wordlists",
    "zondax",
    "zxcvbn"
  ],
  "markdownlint.config": {
    "MD026": false
  },
  "window.title": "${folderPath}",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": true
  },
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "editor.useTabStops": false,
  "breadcrumbs.enabled": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.colorCustomizations": {},
  "editor.formatOnSave": true,
  "vim.camelCaseMotion.enable": true,
  "vim.cursorStylePerMode.normal": "underline",
  "vim.easymotion": true
}
